script/source = "extends Node

var state
var porter
var caster
func stateSetup():
	state = get_parent()
	porter = state.get_node(\"../..\")
	porter.connect(\"turn_end\",self,\"end\")
	caster = state.caster
	if porter.big:
		state.get_node(\"big\").visible = true
		state.get_node(\"sml\").visible = false
		


func end():
	yield(get_tree(),\"idle_frame\")
	BattleMechanics.dealDmg([state.lvl, \"f\"], porter, state.caster, true)
	BattleMechanics.procDown(state, floor(state.lvl/2))

"

