script/source = "extends Node

var state
var porter
func stateSetup():
	state = get_parent()
	porter = state.get_node(\"../..\")
	state.connect(\"tree_exiting\",self,\"expire\")
	lvlUpdate()
	

var bonus = 1
func lvlUpdate():
	porter.dmgReceiveMltGeneral /= bonus
	bonus = 1 - 0.1 * state.lvl
	porter.dmgReceiveMltGeneral *= bonus

func expire():
	porter.dmgReceiveMltGeneral = 1
"

