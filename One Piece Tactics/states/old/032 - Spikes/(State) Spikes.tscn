script/source = "extends Node

var state
var porter
func stateSetup():
	state = get_parent()
	porter = state.get_node(\"../..\")
	porter.connect(\"turn_end\",self,\"end\")
	porter.connect(\"dmg_taken\",self,\"spike\")

var count = 1
func end():
	if count == 0:
		BattleMechanics.removeState(state)
	else:
		count -= 1

func spike(_1,_2,before):
	if before:
		var rad = BattleMechanics.getCellInRadius(porter.posit, state.lvl, false, 0, [], 0, true)
		var cells = BattleMechanics.orderAoE(rad)
		var keys = cells.keys()
		keys.sort()
		
		for i in keys:
			if i>0:
				for j in cells[i]:
					var tgt = BattleMechanics.getEntityAt(j)
					if not tgt or (tgt and tgt.player):
						BattleMechanics.playAnim(state.visuals[0],j)
						if tgt:
							BattleMechanics.dealDmg([3,\"n\"],tgt,porter,true)
				yield(get_tree().create_timer(0.1),\"timeout\")
"

