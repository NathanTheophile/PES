script/source = "extends Node

var state
var porter
func stateSetup():
	state = get_parent()
	porter = state.get_node(\"../..\")
	porter.connect(\"turn_start\", self, \"warn\")
	porter.connect(\"turn_end\", self, \"warn\")

func warn():
	var cells=[]
	for i in BattleMechanics.area.tilemap.get_used_cells().size():
		if BattleMechanics.area.tilemap.get_used_cells()[i].x == porter.posit.x or BattleMechanics.area.tilemap.get_used_cells()[i].y == porter.posit.y:
			cells.append(BattleMechanics.area.tilemap.get_used_cells()[i])
	for i in cells.size():
		BattleMechanics.playAnim(
			state.visuals[0],
			cells[i],
			false,
			\"under\"
		)

func affect():
	var cells=[]
	for i in BattleMechanics.area.tilemap.get_used_cells().size():
		if BattleMechanics.area.tilemap.get_used_cells()[i].x == porter.posit.x or BattleMechanics.area.tilemap.get_used_cells()[i].y == porter.posit.y:
			cells.append(BattleMechanics.area.tilemap.get_used_cells()[i])
	var cellsGroups = {}
	
	for i in cells.size():
		if cellsGroups.has(BattleMechanics.distanceTo(porter.posit, cells[i])):
			cellsGroups[BattleMechanics.distanceTo(porter.posit, cells[i])].append(cells[i])
		else:
			cellsGroups[BattleMechanics.distanceTo(porter.posit, cells[i])] = [cells[i]]
	
	var keys = cellsGroups.keys()
	keys.sort()
	for i in keys.size():
		for j in cellsGroups[keys[i]].size():
			BattleMechanics.playAnim(
				state.visuals[1],
				cellsGroups[keys[i]][j]
				
			)
		yield(get_tree().create_timer(0.1),\"timeout\")
"

