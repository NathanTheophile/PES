[sub_resource type="GDScript" id=30]
script/source = "extends Node

export(Array,String,MULTILINE) var text
func _ready():
	BattleMechanics.connect(\"battleStart\",self,\"setup\")
func setup():
	BattleMechanics.area.ui.crowns.setup(get_parent().trophies,text)
	for i in BattleMechanics.area.entities.get_children():
		if i.ent_name == \"Captain Morgan\":
			i.connect(\"killed\",self,\"end\")
		elif i.player:
			i.connect(\"killed\",self,\"trophy3\")

func end(_a):
	if BattleMechanics.currentRound > 7:
		trophy2()
	for i in BattleMechanics.area.entities.get_children():
		if not i.player and not i.ent_name == \"Captain Morgan\":
			trophy1()
			continue
		if i.player and float(i.hp - i.hp_loss)/i.hp < 0.8:
			trophy3()
	

func trophy1():
	BattleMechanics.area.trophies[0] = false
	BattleMechanics.area.ui.crowns.fail(0)

func trophy2():
	BattleMechanics.area.trophies[1] = false
	BattleMechanics.area.ui.crowns.fail(1)

func trophy3():
	BattleMechanics.area.trophies[2] = false
	BattleMechanics.area.ui.crowns.fail(2)
"

