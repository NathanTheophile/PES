[sub_resource type="GDScript" id=25]
script/source = "extends Node

var entity


func ai_loop():
	entity = get_parent()

	if entity.hp-entity.hp_loss<=0:
			yield()
	yield(get_tree().create_timer(0.1),\"timeout\")
	
	
	yield(BattleMechanics.aiUseSkills(
			entity.skillsUsable,
		0.5),\"completed\")
	
	
	yield(BattleMechanics.turnEndRun(entity), \"completed\")
	
	entity.turn_to(BattleMechanics.findDirectionToLook(entity))
	
	yield(get_tree().create_timer(1),\"timeout\")

	BattleMechanics.enemyEnd(entity)
"

