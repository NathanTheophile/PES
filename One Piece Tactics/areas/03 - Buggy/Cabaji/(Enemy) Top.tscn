[sub_resource type="GDScript" id=25]
script/source = "extends Node

var entity

func ai_loop():
	entity = get_parent()
	
	yield(get_tree().create_timer(0.05),\"timeout\")
	
	yield(BattleMechanics.walk(entity, BattleMechanics.findPathtoRun(entity,false)), \"completed\")
	
	yield(get_tree().create_timer(0.05),\"timeout\")
	
	entity.turn_to(BattleMechanics.findDirectionToLook(entity))

	yield(get_tree().create_timer(0.05),\"timeout\")
	
	BattleMechanics.enemyEnd(entity)
"

