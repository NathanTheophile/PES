[sub_resource type="GDScript" id=1]
script/source = "extends Node

var state
var porter
func stateSetup():
	state = get_parent()
	porter = state.get_node(\"../..\")
	connect(\"tree_exiting\",self,\"out\")
	lvlUpdate()

var mod = 1
var mod2 = 0
func lvlUpdate():
	porter.dmgModMltGeneral /= mod
	porter.mp -= mod2
	
	mod =	 1 + state.lvl * 0.5
	mod2 =	 state.lvl * 2
	
	for i in porter.skillsUsable:
		match i.skillName:
			\"Purrsuit\":
				i.rangeMax = 3 + state.lvl * 3
			\"Meowthquake\":
				i.aoeSize = 2 + state.lvl * 2
	porter.dmgModMltGeneral *= mod
	porter.mp += mod2

func out():
	porter.dmgModMltGeneral /= mod
	porter.mp -= mod2
"

