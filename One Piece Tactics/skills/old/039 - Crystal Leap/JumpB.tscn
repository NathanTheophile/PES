script/source = "extends Node

var parts = []

func _ready():
	
	for i in $\"../Node2D\".get_child_count():
		if $\"../Node2D\".get_child(i).name != \"body\":
			parts.append($\"../Node2D\".get_child(i))
	
	assume(BattleMechanics.currentPlaying.get_node(\"Crystals\").currentElm)

func assume(element):
	var color:Color
	match element:
		\"f\":
			color = Color(1, 0.4, 0.4)
			#ff6666
			pass
		\"e\":
			color = Color(0.529412, 1, 0.4)
			#87ff66
			pass
		\"a\":
			color = Color(1, 0.560784, 0.933333)
			#ff8fee
			pass
		\"w\":
			color = Color(0.317647, 0.776471, 1)
			#51c6ff
			pass
	
	for i in parts.size():
		parts[i].self_modulate = color

"

